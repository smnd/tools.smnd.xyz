{
  "name": "tools",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for small tools and utilities",
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "pnpm --filter qr dev",
    "dev:qr": "pnpm --filter qr dev",
    "dev:qr-code": "pnpm --filter qr-code dev",
    "dev:mx-checker": "pnpm --filter mx-checker dev",
    "build": "pnpm --filter qr build",
    "build:qr": "pnpm --filter qr build",
    "build:qr-code": "pnpm --filter qr-code build",
    "build:mx-checker": "pnpm --filter mx-checker build",
    "build:all": "pnpm --recursive --filter './apps/*' build",
    "preview": "pnpm --filter qr preview",
    "preview:qr": "pnpm --filter qr preview",
    "preview:qr-code": "pnpm --filter qr-code preview",
    "preview:mx-checker": "pnpm --filter mx-checker preview"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {}
}
